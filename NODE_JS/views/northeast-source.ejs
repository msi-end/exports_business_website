<body>
  <%- include('./partials/header.ejs') %>

  <!-- Hero Section -->
  <section class="organic-section">
    <span class="blur_bg rn-blur top-0 right-0"></span>
    <div class="line-f">
      <img src="/image/ricecorn/line.png" alt="" srcset="" />
    </div>
    <span class="blur_bg position-absolute top-0 left-0 "></span>
    <div class="organic-content" data-aos="fade-up">
      <h2 class="heading-primary">Sources from</h2>
      <h1 class="text-transparent">
        <span class="rice-text">Northeast</span>
      </h1>
      <p class="h-desc">
        <span style="font-weight: 600">We farm, export, import...</span> and
        we also train and support. We are on the ground with growers,
        empowering them to leave poverty ehind and restore ecosystems.
      </p>
    </div>

    <div class="image-container" data-aos="fade-up">
      <img src="/image/sourcenortheast/hero-ns.png" alt="Rice crops" class="circle-image" />
    </div>

  </section>

  <!-- Round Number section -->

  <section class="rn-container" data-aos="fade-up">
    <span class="blur_bg rn-blur bottom-0 right-0"></span>
    <div class="dotted-line">
      <img src="/image/sourcenortheast/rn-line.svg" alt="" />
    </div>

    <div class="circles-container">
      <div class="rn-first-row">
        <div class="circle-item">
          <div class="circle-wrapper">
            <div class="rn-circle-image">
              <img src="/image/sourcenortheast/round-1.png" alt="Equitable workplace" />
            </div>
            <div class="number-circle">1</div>
          </div>
          <div class="description">
            Ensure an equitable workplace and good working conditions
          </div>
        </div>

        <div class="circle-item">
          <div class="circle-wrapper">
            <div class="rn-circle-image">
              <img src="/image/sourcenortheast/round-1.png" alt="Smallholder farmers" />
            </div>
            <div class="number-circle">2</div>
          </div>
          <div class="description">
            Listen to and empower smallholder farmers
          </div>
        </div>
      </div>

      <div class="rn-second-row">
        <div class="circle-item">
          <div class="circle-wrapper">
            <div class="rn-circle-image">
              <img src="/image/sourcenortheast/round-1.png" alt="Empower locals" />
            </div>
            <div class="number-circle">3</div>
          </div>
          <div class="description">Listen to and empower locals</div>
        </div>
        <div class="circle-item">
          <div class="circle-wrapper">
            <div class="rn-circle-image">
              <img src="/image/sourcenortheast/round-1.png" alt="Equal partnership" />
            </div>
            <div class="number-circle">4</div>
          </div>
          <div class="description">
            Build equal partnership and co-create impact
          </div>
        </div>
      </div>
    </div>
  </section>
  <span class="blur_bg position-absolute block hero-background-1"></span>
  <!-- Descript section with Wheel -->
  <section class="w-container" data-aos="fade-up">
    <div class="certifications">
      <div class="certification-wheel">
